{
  "name": "learn-express",
  "version": "1.0.0",
  "description": "Learn Nodejs Express",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!vendor/**/*.{js,jsx}"
    ],
    "maxConcurrency": 2
  },
  "keywords": [],
  "author": "Rifki Muhazzar",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "mustache-express": "^1.3.2",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
